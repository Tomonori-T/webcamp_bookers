<h1>Editing Book</h1>
<%= form_with model:@book, url:"/books/#{@book.id}", local:true do |f| %>
<p>
  <h6>Title</h6>
  <%= f.text_field :title %>
</p>
<p>
  <h6>Body</h6>
  <%= f.text_area :body %>
</p>
<p>
<%= f.submit 'update' %>
</p>
<td>
  <%= link_to "Show", book_path(@book.id) %>
  |
  <a href = "/books">Back</a>
</td>
<% end %>